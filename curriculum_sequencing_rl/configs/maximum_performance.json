{
  "environment": {
    "data_path": "../preprocessed_kt_data.csv",
    "reward_correct_w": 1.0,
    "reward_score_w": 0.0,
    "seed": 123
  },
  "models": ["ql", "dqn", "a2c", "a3c", "ppo"],
  "q_learning": {
    "epochs": 10,
    "alpha": 0.15,
    "gamma": 0.95,
    "eps_start": 0.4,
    "eps_end": 0.0,
    "eps_decay_epochs": 6,
    "select_best_on_val": true,
    "val_episodes": 500
  },
  "dqn": {
    "episodes": 500,
    "lr": 2e-4,
    "gamma": 0.997,
    "batch_size": 512,
    "buffer_size": 150000,
    "hidden_dim": 512,
    "eps_start": 1.0,
    "eps_end": 0.005,
    "eps_decay_steps": 25000,
    "target_tau": 0.005,
    "target_update_interval": 1
  },
  "a2c": {
    "episodes": 500,
    "lr": 2e-4,
    "entropy_coef": 0.015,
    "value_coef": 0.6,
    "bc_warmup": 12,
    "bc_weight": 3.0,
    "batch_episodes": 8
  },
  "a3c": {
    "episodes": 500,
    "lr": 2e-4,
    "entropy_coef": 0.015,
    "value_coef": 0.6,
    "gae_lambda": 0.97,
    "bc_warmup": 12,
    "bc_weight": 3.0,
    "rollouts": 8
  },
  "ppo": {
    "episodes": 500,
    "lr": 2e-4,
    "clip_eps": 0.15,
    "epochs": 10,
    "batch_episodes": 20,
    "minibatch_size": 2048,
    "entropy_coef": 0.015,
    "value_coef": 0.6,
    "gae_lambda": 0.97,
    "bc_warmup": 12,
    "bc_weight": 3.0
  },
  "eval_episodes": 1500,
  "include_chance": true,
  "include_trivial": true,
  "include_markov": true,
  "metrics_csv": "maximum_performance_results.csv"
}
