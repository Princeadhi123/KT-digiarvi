{
  "environment": {
    "data_path": "../preprocessed_kt_data.csv",
    "reward_correct_w": 1.0,
    "reward_score_w": 0.0,
    "seed": 123
  },
  "models": ["dqn", "ppo"],
  "dqn": {
    "episodes": 400,
    "learning_rate": 2e-4,
    "gamma": 0.997,
    "batch_size": 512,
    "buffer_size": 120000,
    "hidden_dim": 512,
    "eps_start": 1.0,
    "eps_end": 0.01,
    "eps_decay_steps": 20000,
    "target_tau": 0.01,
    "target_update_interval": 1
  },
  "ppo": {
    "episodes": 400,
    "learning_rate": 3e-4,
    "clip_eps": 0.2,
    "epochs": 8,
    "batch_episodes": 16,
    "minibatch_size": 1024,
    "entropy_coef": 0.02,
    "value_coef": 0.5,
    "gae_lambda": 0.95,
    "bc_warmup": 8,
    "bc_weight": 2.0
  },
  "eval_episodes": 1000,
  "include_chance": true,
  "include_trivial": true,
  "include_markov": true,
  "metrics_csv": "high_performance_results.csv"
}
